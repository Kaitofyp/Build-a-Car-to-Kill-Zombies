local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("KAITO HUB", "Default")

local tab = DrRayLibrary.newTab("Main", "ImageIdHere")

tab.newButton("INF MONEY", "Click me | ---RISK---", function()
    local args = {
    [1] = "PlasticBlock",
    [2] = -1000000000
}

game:GetService("ReplicatedStorage").RemoteEvents.ShopHandlerRemotes.PurchaseItem:InvokeServer(unpack(args))
end)

